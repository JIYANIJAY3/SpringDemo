<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="employeebean" class="com.inexture.springDemo.bean.EmployeeBean">

		<property name="name" value="Jay" />

		<property name="phoneNumbers">
			<list>
				<value>9909398974</value>
				<value>9601986366</value>
				<value>6353948013</value>
				<value>9924996244</value>
				<value>9909398991</value>
			</list>
		</property>

		<property name="address">
			<set>
				<value>Ahmedabad</value>
				<value>Amreli</value>
				<value>Surat</value>
				<value>Rajkot</value>
			</set>
		</property>

		<property name="courses">
			<map>
				<entry key="java" value="1 month" />
				<entry key="c++" value="2 month" />
				<entry key="javascript" value="3 month" />
			</map>
		</property>

	</bean>

</beans>