<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.3.xsd 
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.3.xsd">

	<context:annotation-config />

	<bean name="addressbean1"
		class="com.inexture.springDemo.autowire.annotation.AddressBean">
		<property name="country" value="india" />
		<property name="city" value="ahmedabad" />
		<property name="pincode" value="380006" />
	</bean>
	<bean name="addressbean"
		class="com.inexture.springDemo.autowire.annotation.AddressBean">
		<property name="country" value="india" />
		<property name="city" value="surat" />
		<property name="pincode" value="365450" />
	</bean>

	<bean name="userbean"
		class="com.inexture.springDemo.autowire.annotation.UserBean">
		<property name="name" value="Jay" />
	</bean>
</beans>