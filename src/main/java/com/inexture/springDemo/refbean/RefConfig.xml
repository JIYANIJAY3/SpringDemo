<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean name="userbean"
		class="com.inexture.springDemo.refbean.UserBean">
		<property name="firstName" value="Jay" />
		<property name="lastName" value="Jiyani" />
		<property name="addressBean">
			<ref bean="addressbean" />
		</property>
	</bean>
	
	<bean name="addressbean"
		class="com.inexture.springDemo.refbean.AddressBean">
		<property name="country" value="india" />
		<property name="state" value="gujarat" />
		<property name="city" value="Ahmedabad" />
		<property name="pincode" value="380006" />
	</bean>


</beans>